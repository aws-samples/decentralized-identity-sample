{
    "@context": [
      "https://w3id.org/openbadges/v2",
      "https://w3id.org/blockcerts/v2"
    ],
    "type": "Profile",
    "id": "<cloudfront endpoint>/issuer-profile.json",
    "name": "Issuer Credential",
    "url": "https://<cloudfront endpoint>",
    "introductionURL": "<cloudfront endpoint>/intro/",
    "revocationList": "<cloudfront endpoint>/revocation-list.json",
    "publicKey": [
      {
        "id": "ecdsa-koblitz-pubkey:<issuer wallet address>",
        "created": "<issuer register datetime>"
      }
    ],
    "email": "<issuer email address>"
  }